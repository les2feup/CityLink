{
    "citylink": "https://example.org/CityLink#",
    "manifest": {
        "@id": "citylink:manifest",
        "@type": "@id",
        "@context": {
            "cl": "citylink",
            "placeholder": {
                "@id": "cl:hasPlaceholder",
                "@container": "@index"
            },
            "source": {
                "@id": "cl:hasSource",
                "@container": "@set",
                "@context": {
                    "filepath": "cl:hasFilePath",
                    "href": {
                        "@id": "cl:hasHref",
                        "@type": "@id"
                    },
                    "contentType": "cl:hasContentType",
                    "sha256": {
                        "@id": "cl:hasHash",
                        "@type": "cl:sha256"
                    }
                }
            }
        }
    }
}
